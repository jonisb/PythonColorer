<?xml version="1.0" encoding="iso-8859-1"?>
<type name="python36">
	&py36NUMBER;
	&py36NAME;

<!--atom:
(
	'(' [yield_expr|testlist_comp] ')'
	|
	'[' [testlist_comp] ']'
	|
	'{' [dictorsetmaker] '}'
	|
	NAME | NUMBER | STRING+ | '...'
)
-->
    <scheme name="atom">
		<inherit scheme="NUMBER"/>
		<inherit scheme="NAME"/>
    </scheme>

	<scheme name="python36">
		<inherit scheme="atom"/>
		<regexp match="/\S/" region="def:Error" priority="low"/>
	</scheme>
</type>
